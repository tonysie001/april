	</test>
	<!-- 班长注销 -->
	<test name="clalogout3">
		<classes>
			<class name="com.soulinfo.cases.LoginCase">
				<methods>
					<include name="testLogout"></include>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 关闭浏览器 -->
	<test name="close">
		<classes>
			<class name="com.soulinfo.cases.BrowserCase">
				<methods>
					<include name="closeBrowser"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite> 