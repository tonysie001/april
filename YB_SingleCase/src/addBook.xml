<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="YB">
	<parameter name="code" value="31e30d4b504"></parameter> 
<!-- 打开浏览器 -->
	<test name="open">
		<classes>
			<class name="com.cases.BrowserCase">
			<methods >
					<include name="openBrowser"></include>
					<exclude name="closeBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
<!-- 登录 -->	
	<test name="login">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testLogin"></include>
				</methods>
			</class>
		</classes>
	</test>
	
	<!-- 加入新书 -->	
	<test name="addBook">
		<classes>
			<class name="com.cases.AddBookCase">
				<methods>
					<include name="test1MyBook"></include>
					<include name="test2AddBook"></include>
				</methods>
			</class>
		</classes>
	</test>
	
	
	<!-- 注销 -->
	<test name="logout">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testLogout"></include>
				</methods>
			</class>
		</classes>
	</test>
<!-- 关闭浏览器 -->
		<test name="close">
		<classes>
			<class name="com.cases.BrowserCase">
				<methods >
					<include name="closeBrowser"></include>
					<exclude name="openBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite> 