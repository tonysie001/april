<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="YB">
<!-- 打开浏览器 -->
	<test name="open">
		<classes>
			<class name="com.cases.BrowserCase">
			<methods >
					<include name="openBrowser"></include>
					<exclude name="closeBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
<!-- 登录 -->	
	<test name="login">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testLogin"></include>
					<include name="testLogout"></include>
				</methods>
			</class>
		</classes>
	</test>
<!-- 关闭浏览器 -->
		<test name="close">
		<classes>
			<class name="com.cases.BrowserCase">
				<methods >
					<include name="closeBrowser"></include>
					<exclude name="openBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite> 