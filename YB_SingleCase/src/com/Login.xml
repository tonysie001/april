<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="YB"  junit="false" verbose="3" parallel="false">
	<test name="open" junit="false" verbose="3" parallel="false"
		thread-count="5" annotations="javadoc" time-out="10000" enabled="true"
		skipfailedinvocationcounts="true" preserve-order="true"
		allow-return-values="true">
		<classes>
			<class name="com.cases.BrowserCase">
				<methods>
					<include name="openBrowser"></include>
					<exclude name="closeBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
	<test name="login" junit="false" verbose="3" parallel="false"
		thread-count="5" annotations="javadoc" time-out="10000" enabled="true"
		skipfailedinvocationcounts="true" preserve-order="true"
		allow-return-values="true">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testLogin"></include>
					<exclude name="testL"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite> 