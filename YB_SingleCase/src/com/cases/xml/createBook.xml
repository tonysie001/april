<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="YB" allow-return-values="true">
	<!-- 打开浏览器 -->
	<test name="open">
		<classes>
			<class name="com.cases.BrowserCase">
				<methods>
					<include name="openBrowser"></include>
					<exclude name="closeBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 班长登录 -->
	<test name="clalogin">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testClaLogin"></include>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 班长创建新书册 -->
	<test name="createBook">
		<classes>
			<class name="com.cases.CreateBooksCase">
				<methods>
					<include name="test1CreateBooks"></include>		
					<include name="test2Code"></include>
					<include name="test3code"></include>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 班长创建新投票 -->
		<test name="createVote">
		<classes>
			<class name="com.cases.CreateVoteCase">
				<methods>
					<include name="testCreateVote"></include>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 注销 -->
	<test name="clalogout">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testLogout"></include>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 关闭浏览器 -->
	<test name="close">
		<classes>
			<class name="com.cases.BrowserCase">
				<methods>
					<include name="closeBrowser"></include>
					<exclude name="openBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite> 