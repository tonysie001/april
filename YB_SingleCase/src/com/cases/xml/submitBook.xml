<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="YB">
<!-- 打开浏览器 -->
	<test name="open">
		<classes>
			<class name="com.cases.BrowserCase">
			<methods >
					<include name="openBrowser"></include>
					<exclude name="closeBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
<!-- 班长登录 -->	
	<test name="login">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testClaLogin"></include>
				</methods>
			</class>
		</classes>
	</test>
	
	<!-- 打开创建新书 -->	

	<test name="openCreateBook">
		<classes>
			<class name="com.cases.AddBookCase">
				<methods>			
					<include name="testOpenCreateBook"></include>
					<exclude name="testMyBook"></exclude>
					<exclude name="testOpenBook"></exclude>
					<exclude name="testAddBook"></exclude>
				</methods>
			</class>
		</classes>
	</test>
	<!-- 班长提交书册 -->
	<test name="classSubmitBook">
		<classes>
			<class name="com.cases.SubmitBookCase">
				<methods>				
					<include name="testSubmitBook"></include>		
				</methods>
			</class>
		</classes>
	</test>
	<!-- 注销 -->
	<test name="logout">
		<classes>
			<class name="com.cases.LoginCase">
				<methods>
					<include name="testLogout"></include>
				</methods>
			</class>
		</classes>
	</test>
<!-- 关闭浏览器 -->
		<test name="close">
		<classes>
			<class name="com.cases.BrowserCase">
				<methods >
					<include name="closeBrowser"></include>
					<exclude name="openBrowser"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite> 